#include <iostream>
using namespace std;
int main () {
 
    int pass[6];
    int i;
    
    cout <<"kode yang dimasukan :";
    cin >> pass [i] ;
    
    int password [10] ;
    for (int i=0 ; i<6 ; i++)
    {
        if ( (pass [i] == ' ')
        {
            pass [i] == '1';
        }
        else if ( (pass [i] == 'a') || (pass [i] =='b') || (pass [i]=='c') )
        {
            pass [i] == '2';
        }
        
        else if ( (pass [i] == 'd') || (pass [i] =='e') || (pass [i]=='f') )
        {
            pass [i] == '3';
        }
        
        else if ( (pass [i] == 'g') || (pass [i] =='h') || (pass [i]=='i') )
        {
            pass [i] == '4';
        }
        
        else if ( (pass [i] == 'j') || (pass [i] =='k') || (pass [i]=='l') )
        {
            pass [i] == '5';
        }
        
        else if ( (pass [i] == 'm') || (pass [i] =='n') || (pass [i]=='o') )
        {
            pass [i] == '6';
        }
     
        else if ( (pass [i] == 'p') || (pass [i] =='q') || (pass [i]=='r') )
        {
            pass [i] == '7';
        }
        else if ( (pass [i] == 's') || (pass [i] =='t') || (pass [i]=='u') )
        {
            pass [i] == '8';
        }
        
        else if ( (pass [i] == 'v') || (pass [i] =='w') || (pass [i]=='x') )
        {
            pass [i] == '9';
        }
        
        else if ( (pass [i] == 'y') || (pass [i] =='z'))
        {
            pass [i] == '10';
        }
    } //lanjut terus sampai 9
    cout <<"kode yang disimpan :" ;
    cout<<password;

 return 0 ;      
    }
